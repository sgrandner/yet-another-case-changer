{
	"name": "yet-another-case-changer",
	"displayName": "Yet Another Case Changer",
	"description": "Change/toggle the case of the selected word(s)",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.63.0"
	},
	"categories": [
		"Formatters"
	],
	"activationEvents": [
		"onCommand:yet-another-case-changer.upper-snake-case"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "yet-another-case-changer.upper-snake-case",
				"title": "Yet Another Case Changer: Upper Snake Case"
			},
			{
				"command": "yet-another-case-changer.upper-kebap-case",
				"title": "Yet Another Case Changer: Upper Kebap Case"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.63.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.9.1",
		"@typescript-eslint/parser": "^5.9.1",
		"eslint": "^8.6.0",
		"glob": "^7.2.0",
		"mocha": "^9.1.3",
		"typescript": "^4.5.4",
		"ts-loader": "^9.2.6",
		"webpack": "^5.66.0",
		"webpack-cli": "^4.9.1",
		"@vscode/test-electron": "^2.0.3"
	}
}
